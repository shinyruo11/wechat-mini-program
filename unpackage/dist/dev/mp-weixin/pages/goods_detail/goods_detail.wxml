<view class="goods-detail"><block wx:if="{{goods_detail.pics.length>0}}"><swiper class="swiper" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{2500}}" duration="{{500}}" circular="true" indicator-active-color="#DC4D41" indicator-color="white"><block wx:for="{{goods_detail.pics}}" wx:for-item="item" wx:for-index="__i0__" wx:key="pics_id"><swiper-item><image class="swiper-img" src="{{item.pics_mid_url}}"></image></swiper-item></block></swiper></block><block wx:else><image src="https://ww1.sinaimg.cn/large/007rAy9hgy1g24by9t530j30i20i2glm.jpg" width="100%"></image></block><view class="detail-title"><view class="title-price"><view class="price-left"><text class="price-icon">￥</text><text class="price-number">{{goods_detail.goods_price}}</text></view><view class="price-right"><block wx:if="{{isCollect==false}}"><image src="../../static/collect.png" data-event-opts="{{[['tap',[['collect',['$0'],['goods_detail']]]]]}}" bindtap="__e"></image></block><block wx:if="{{isCollect==true}}"><image src="../../static/collect-active.png" data-event-opts="{{[['tap',[['collect',['$0'],['goods_detail']]]]]}}" bindtap="__e"></image></block></view></view><view class="title-name">{{goods_detail.goods_name}}</view></view><view class="divider"></view><van-divider vue-id="c64ca100-1" contentPosition="center" bind:__l="__l" vue-slots="{{['default']}}">详情</van-divider><view class="specification"><view class="specification-title">规格参数</view><van-cell-group vue-id="c64ca100-2" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{goods_detail.attrs}}" wx:for-item="item" wx:for-index="__i1__" wx:key="attr_id"><van-cell vue-id="{{('c64ca100-3-'+__i1__)+','+('c64ca100-2')}}" title="{{item.attr_name}}" value="{{item.attr_vals}}" bind:__l="__l"></van-cell></block></van-cell-group><view class="rich-text"><rich-text nodes="{{goods_detail.goods_introduce}}"></rich-text></view><block wx:for="{{goods_detail.pics}}" wx:for-item="item" wx:for-index="__i2__" wx:key="pics_id"><image class="specification-image" src="{{item.pics_mid_url}}"></image></block></view><van-toast vue-id="c64ca100-4" id="van-toast" bind:__l="__l"></van-toast><view class="goods-action"></view><van-goods-action vue-id="c64ca100-5" bind:__l="__l" vue-slots="{{['default']}}"><van-goods-action-icon vue-id="{{('c64ca100-6')+','+('c64ca100-5')}}" icon="chat-o" text="客服" bind:__l="__l"></van-goods-action-icon><navigator url="../cart/cart" open-type="switchTab"><van-goods-action-icon vue-id="{{('c64ca100-7')+','+('c64ca100-5')}}" icon="cart-o" text="购物车" info="{{cartList_length}}" bind:__l="__l"></van-goods-action-icon></navigator><van-goods-action-button vue-id="{{('c64ca100-8')+','+('c64ca100-5')}}" text="加入购物车" type="warning" data-event-opts="{{[['^tap',[['addCart']]]]}}" bind:tap="__e" bind:__l="__l"></van-goods-action-button><van-goods-action-button vue-id="{{('c64ca100-9')+','+('c64ca100-5')}}" text="立即购买" data-event-opts="{{[['^tap',[['goOrder']]]]}}" bind:tap="__e" bind:__l="__l"></van-goods-action-button></van-goods-action></view>
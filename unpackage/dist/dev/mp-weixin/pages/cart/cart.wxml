<view class="cart"><block wx:if="{{cartList.length>0}}"><view class="cart-list"><checkbox-group data-event-opts="{{[['change',[['onChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{cartList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="goods_id"><view class="cart-group"><checkbox style="display:flex;align-items:center;transform:scale(0.8);" value="{{item.goods_id}}" checked="{{item.checked}}" color="red" data-event-opts="{{[['tap',[['change',['$0'],[[['cartList','goods_id',item.goods_id]]]]]]]}}" bindtap="__e"></checkbox><view class="group-left"><image src="{{item.goods_big_logo?item.goods_big_logo:'https://ww1.sinaimg.cn/large/007rAy9hgy1g24by9t530j30i20i2glm.jpg'}}"></image></view><view class="group-right"><view class="group-name">{{item.goods_name}}</view><view class="group-price"><text class="price-icon">￥</text><text class="price-number">{{item.goods_price}}</text><view class="number"><view data-event-opts="{{[['tap',[['subNumber',['$0'],[[['cartList','goods_id',item.goods_id]]]]]]]}}" bindtap="__e">-</view><view class="num">{{item.num}}</view><view data-event-opts="{{[['tap',[['addNumber',['$0'],[[['cartList','goods_id',item.goods_id]]]]]]]}}" bindtap="__e">+</view></view></view></view></view></block></checkbox-group></view></block><view style="height:110rpx;"></view><block wx:if="{{isShow}}"><view class="sumbit-order"><checkbox-group data-event-opts="{{[['tap',[['AllCheck',['$event']]]]]}}" bindtap="__e"><checkbox style="display:flex;align-items:center;" checked="{{isAllChecked}}" color="red"></checkbox></checkbox-group><view class="text"><text class="all">合计:</text><text class="price-icon">￥</text><text class="price-number">{{totalPrice}}</text></view><view data-event-opts="{{[['tap',[['goOrder',['$event']]]]]}}" class="button" bindtap="__e">{{"去结算("+totalNum+")"}}</view></view></block><block wx:if="{{isEmpty}}"><van-empty class="search" vue-id="1775f8c0-1" image="search" description="购物车空空的" bind:__l="__l"></van-empty></block><van-toast vue-id="1775f8c0-2" id="van-toast" bind:__l="__l"></van-toast><van-dialog vue-id="1775f8c0-3" id="van-dialog" bind:__l="__l"></van-dialog></view>